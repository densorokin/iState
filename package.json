{
  "name": "state-machine-lib",
  "version": "1.0.13-08",
  "description": "This is an NPM package to manage react state by state machine pattern",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "scripts": {
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production && npx dts-bundle-generator -o build/main.d.ts src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest"
  },
  "keywords": [
    "react",
    "hook",
    "state machine"
  ],
  "author": "Denis Sorokin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/densorokin/state-machine.git"
  },
  "bugs": {
    "url": "https://github.com/densorokin/state-machine.git/issues"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-dts-webpack-plugin": "^1.3.12",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  }
}